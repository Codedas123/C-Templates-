
// Print Binary //

void printBinary(int num){
for(int i=10; i>=0; --i){
cout<<((num>>i) &1);
}
cout<<endl;
}

// check set bit //

if(a& 1<<i))!=0){
cout<<"set bit";
}
else
 cout<< "not set bit";
 
 // set a bit //
 
 printBinary(a | (1<<i));
 
 // unset a bit //
 
 printBinary(a & ~(1<<i));
 
 // number of set bits //
 __builtin_popcount(a);
 
